{
  "name": "russian-js-transpiler",
  "version": "1.0.1",
  "description": "A transpiler that converts Russian JavaScript-like code to standard JavaScript",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "vite build",
    "start": "vite",
    "dev": "vite",
    "preview": "vite preview --port 3001",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs --port 3002",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "example": "node dist/cli.js examples/example.rjs examples/example.js"
  },
  "bin": {
    "russian-js-transpiler": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nike-17/rjs"
  },
  "keywords": [
    "transpiler",
    "russian",
    "javascript",
    "compiler",
    "language",
    "translation"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nike-17/rjs/issues"
  },
  "homepage": "https://github.com/nike-17/rjs#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "commander": "^9.0.0"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.6.8",
    "@types/babel__template": "^7.4.4",
    "@types/babel__traverse": "^7.20.5",
    "@types/estree": "^1.0.8",
    "@types/graceful-fs": "^4.1.8",
    "@types/hast": "^3.0.4",
    "@types/istanbul-lib-coverage": "^2.0.6",
    "@types/istanbul-lib-report": "^3.0.3",
    "@types/istanbul-reports": "^3.0.4",
    "@types/jest": "^29.5.12",
    "@types/linkify-it": "^5.0.0",
    "@types/markdown-it": "^14.1.2",
    "@types/mdast": "^4.0.4",
    "@types/mdurl": "^2.0.0",
    "@types/node": "^20.11.19",
    "@types/prettier": "^3.0.0",
    "@types/stack-utils": "^2.0.3",
    "@types/unist": "^3.0.3",
    "@types/web-bluetooth": "^0.0.21",
    "@types/yargs": "^17.0.34",
    "@types/yargs-parser": "^21.0.3",
    "@vitejs/plugin-vue": "^6.0.2",
    "gh-pages": "^6.1.1",
    "jest": "^29.7.0",
    "lucide-vue-next": "^0.554.0",
    "terser": "^5.29.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.4.0",
    "vitepress": "^1.6.4"
  }
