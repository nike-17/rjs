{
  "name": "russian-js-transpiler",
  "version": "1.0.0",
  "description": "A transpiler that converts Russian JavaScript-like code to standard JavaScript",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "example": "node dist/cli.js examples/example.rjs examples/example.js"
  },
  "bin": "./dist/cli.js",
  "files": ["dist"],
  "exports": {
    ".": "./dist/index.js"
  },
  "keywords": ["transpiler", "russian", "javascript", "compiler"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^9.0.0",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0"
  }
}
