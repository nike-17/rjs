{
  "name": "russian-js-transpiler",
  "version": "1.0.0",
  "description": "A transpiler that converts Russian JavaScript-like code to standard JavaScript",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "tsc",
    "start": "node dist/index.js",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "example": "node dist/cli.js examples/example.rjs examples/example.js"
  },
  "bin": {
    "russian-js-transpiler": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nike-17/rjs"
  },
  "keywords": [
    "transpiler",
    "russian",
    "javascript",
    "compiler",
    "language",
    "translation"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yourusername/russian-js-transpiler/issues"
  },
  "homepage": "https://github.com/yourusername/russian-js-transpiler#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@types/node": "^20.0.0",
    "commander": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.45",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "vitepress": "^1.6.4"
  }
}
