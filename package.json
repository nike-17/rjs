{
  "name": "russian-js-transpiler",
  "version": "1.0.0",
  "description": "A transpiler that converts Russian JavaScript-like code to standard JavaScript",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "tsc",
    "start": "node dist/index.js",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "example": "node dist/cli.js examples/example.rjs examples/example.js"
  },
  "bin": {
    "russian-js-transpiler": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nike-17/rjs"
  },
  "keywords": [
    "transpiler",
    "russian",
    "javascript",
    "compiler",
    "language",
    "translation"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yourusername/russian-js-transpiler/issues"
  },
  "homepage": "https://github.com/yourusername/russian-js-transpiler#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "commander": "^9.0.0"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__template": "^7.4.4",
    "@types/babel__traverse": "^7.28.0",
    "@types/estree": "^1.0.8",
    "@types/graceful-fs": "^4.1.9",
    "@types/hast": "^3.0.4",
    "@types/istanbul-lib-coverage": "^2.0.6",
    "@types/istanbul-lib-report": "^3.0.3",
    "@types/istanbul-reports": "^3.0.4",
    "@types/jest": "^27.5.2",
    "@types/linkify-it": "^5.0.0",
    "@types/markdown-it": "^14.1.2",
    "@types/mdast": "^4.0.4",
    "@types/mdurl": "^2.0.0",
    "@types/node": "^17.0.45",
    "@types/prettier": "^2.7.3",
    "@types/stack-utils": "^2.0.3",
    "@types/unist": "^3.0.3",
    "@types/web-bluetooth": "^0.0.21",
    "@types/yargs": "^17.0.34",
    "@types/yargs-parser": "^21.0.3",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "vitepress": "^1.6.4"
  }
}
